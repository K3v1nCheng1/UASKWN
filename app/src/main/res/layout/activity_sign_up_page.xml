<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/main"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".SignUpPage">

    <TextView
        android:id="@+id/textView7"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="160dp"
        android:layout_marginTop="100dp"
        android:layout_marginEnd="160dp"
        android:fontFamily="@font/jblack"
        android:text="Hello!"
        android:textAlignment="center"
        android:textColor="#B9B8FA"
        android:textSize="30sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/textView8"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="96dp"
        android:layout_marginEnd="97dp"
        android:text=" welcome to XXXXXXX app \nsign up to get started."
        android:textAlignment="center"
        android:textColor="#B9B8FA"
        android:textSize="18sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView7" />

    <EditText
        android:id="@+id/yourname"
        android:layout_width="328dp"
        android:layout_height="48dp"
        android:layout_alignParentLeft="true"
        android:layout_alignParentTop="true"
        android:layout_marginStart="41dp"
        android:layout_marginTop="40dp"
        android:layout_marginEnd="42dp"
        android:background="@drawable/frame_57"
        android:fontFamily="@font/rblack"
        android:inputType="text"
        android:text="   Your Name"
        android:textColor="#D9D9D9"
        android:textSize="16sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView8" />

    <EditText
        android:id="@+id/emailsignin"
        android:layout_width="328dp"
        android:layout_height="48dp"
        android:layout_alignParentLeft="true"
        android:layout_alignParentTop="true"
        android:layout_marginStart="41dp"
        android:layout_marginTop="14dp"
        android:layout_marginEnd="42dp"
        android:background="@drawable/frame_57"
        android:fontFamily="@font/rblack"
        android:inputType="text"
        android:text="   Enter email"
        android:textColor="#D9D9D9"
        android:textSize="16sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/yourname" />

    <EditText
        android:id="@+id/inputpw"
        android:layout_width="328dp"
        android:layout_height="48dp"
        android:layout_alignParentLeft="true"
        android:layout_alignParentTop="true"
        android:layout_marginStart="41dp"
        android:layout_marginTop="14dp"
        android:layout_marginEnd="42dp"
        android:background="@drawable/frame_57"
        android:fontFamily="@font/rblack"
        android:text="   Password"
        android:textColor="#D9D9D9"
        android:textSize="16sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/emailsignin" />

    <TextView
        android:id="@+id/privacypolicy"
        android:layout_width="328dp"
        android:layout_height="34dp"
        android:layout_marginStart="42dp"
        android:layout_marginTop="20dp"
        android:layout_marginEnd="41dp"
        android:text="by signing up you agree to our terms and CONDITIONS of use and privacy policy."
        android:textColor="#B9B8FA"
        android:textSize="12sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/inputpw" />

    <Button
        android:id="@+id/frame_60"
        android:layout_width="328dp"
        android:layout_height="48dp"
        android:layout_alignParentLeft="true"
        android:layout_alignParentTop="true"
        android:layout_marginStart="41dp"
        android:layout_marginTop="20dp"
        android:layout_marginEnd="42dp"
        android:background="@drawable/frame_60"
        android:text="Sign Up"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/privacypolicy" />

    <TextView
        android:id="@+id/textView3"
        android:layout_width="155dp"
        android:layout_height="20dp"
        android:layout_alignParentLeft="true"
        android:layout_alignParentTop="true"
        android:layout_marginStart="89dp"
        android:layout_marginTop="10dp"
        android:fontFamily="@font/rblack"
        android:text="Already have an account?"
        android:textColor="#8D93AB"
        android:textSize="13sp"
        app:layout_constraintEnd_toStartOf="@+id/loginBTN"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/frame_60" />

    <TextView
        android:id="@+id/loginBTN"
        android:layout_width="wrap_content"
        android:layout_height="48dp"
        android:layout_marginEnd="106dp"
        android:clickable="true"
        android:focusable="true"
        android:fontFamily="@font/rblack"
        android:padding="10dp"
        android:text="LOGIN"
        android:textColor="#646FD4"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/textView3"
        app:layout_constraintTop_toBottomOf="@+id/frame_60" />

</androidx.constraintlayout.widget.ConstraintLayout>

