<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/main"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".LogInPage">

    <!-- Auto layout, variables, and unit scale are not yet supported -->
    <TextView
        android:id="@+id/textView4"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="118dp"
        android:layout_marginTop="120dp"
        android:layout_marginEnd="107dp"
        android:fontFamily="@font/jblack"
        android:text="Hello Again!"
        android:textAlignment="center"
        android:textColor="#B9B8FA"
        android:textSize="30sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/textView5"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="152dp"
        android:layout_marginTop="17dp"
        android:layout_marginEnd="142dp"
        android:layout_marginBottom="39dp"
        android:fontFamily="@font/jblack"
        android:text="Welcome Back"
        android:textAlignment="center"
        android:textColor="#B9B8FA"
        android:textSize="16sp"
        app:layout_constraintBottom_toTopOf="@+id/email"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView4" />

    <EditText
        android:id="@+id/email"
        android:layout_width="328dp"
        android:layout_height="48dp"
        android:layout_alignParentLeft="true"
        android:layout_alignParentTop="true"
        android:layout_marginStart="41dp"
        android:layout_marginTop="39dp"
        android:layout_marginEnd="42dp"
        android:background="@drawable/frame_57"
        android:fontFamily="@font/rblack"
        android:inputType="text"
        android:text="   Enter your email here"
        android:textColor="#D9D9D9"
        android:textSize="16sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView5" />

    <EditText
        android:id="@+id/input"
        android:layout_width="328dp"
        android:layout_height="48dp"
        android:text="   Password"
        android:layout_alignParentLeft="true"
        android:layout_alignParentTop="true"
        android:layout_marginStart="41dp"
        android:layout_marginTop="27dp"
        android:layout_marginEnd="42dp"
        android:background="@drawable/frame_57"
        android:fontFamily="@font/rblack"
        android:textColor="#D9D9D9"
        android:textSize="16sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/email" />

    <Button
        android:id="@+id/LoginBTN"
        android:layout_width="350dp"
        android:layout_height="48dp"
        android:background="@drawable/frame_60"
        android:backgroundTint="#646FD4"
        android:text="LOG IN"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.491"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.561" />

    <TextView
        android:id="@+id/or_continue"
        android:layout_width="150dp"
        android:layout_height="20dp"
        android:layout_alignParentLeft="true"
        android:layout_alignParentTop="true"
        android:layout_marginStart="147dp"
        android:layout_marginTop="42dp"
        android:layout_marginEnd="114dp"
        android:fontFamily="@font/jblack"
        android:gravity="top"
        android:text="or continue with"
        android:textColor="#888888"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/LoginBTN" />

    <RelativeLayout
        android:id="@+id/frame_62"
        android:layout_width="350dp"
        android:layout_height="40dp"
        android:layout_alignParentLeft="true"
        android:layout_alignParentTop="true"
        android:layout_marginStart="34dp"
        android:layout_marginTop="15dp"
        android:layout_marginEnd="27dp"
        android:background="@drawable/frame_62"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/or_continue">

        <ImageView
            android:id="@+id/imageView"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:paddingLeft="135sp"
            android:paddingTop="8sp"
            android:src="@drawable/logos_google_icon" />

        <TextView
            android:id="@+id/textView6"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:fontFamily="@font/rblack"
            android:gravity="top"
            android:paddingLeft="167sp"
            android:paddingTop="10sp"
            android:text="Google"
            android:textColor="#888888"
            android:textSize="14sp" />

    </RelativeLayout>

    <TextView
        android:id="@+id/not_a_membe"
        android:layout_width="100dp"
        android:layout_height="16dp"
        android:layout_alignParentLeft="true"
        android:layout_alignParentTop="true"
        android:layout_marginStart="124dp"
        android:layout_marginTop="16dp"
        android:layout_marginEnd="6dp"
        android:fontFamily="@font/rblack"
        android:gravity="center_horizontal|top"
        android:text="Not a member?"
        android:textColor="#888888"
        android:textSize="13sp"
        app:layout_constraintEnd_toStartOf="@+id/registerBTN"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/frame_62" />

    <TextView
        android:id="@+id/registerBTN"
        android:layout_height="48dp"
        android:layout_width="wrap_content"
        android:layout_marginTop="16dp"
        android:layout_marginEnd="131dp"
        android:clickable="true"
        android:focusable="true"
        android:fontFamily="@font/rblack"
        android:text="Register"
        android:textColor="#646FD4"
        android:textSize="13sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/frame_62" />



</androidx.constraintlayout.widget.ConstraintLayout>